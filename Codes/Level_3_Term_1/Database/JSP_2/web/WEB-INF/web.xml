<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginProcess</servlet-name>
        <servlet-class>sukarna.servlets.LoginProcess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>sukarna.servlets.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToCart</servlet-name>
        <servlet-class>sukarna.servlets.AddToCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateAccount</servlet-name>
        <servlet-class>sukarna.servlets.CreateAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowStatementServlet</servlet-name>
        <servlet-class>sukarna.servlets.ShowStatementServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>transactionProcess</servlet-name>
        <servlet-class>sukarna.servlets.transactionProcess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginProcess</servlet-name>
        <url-pattern>/LoginProcess.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/HomeServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddToCart</servlet-name>
        <url-pattern>/AddToCart.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateAccount</servlet-name>
        <url-pattern>/CreateAccount.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowStatementServlet</servlet-name>
        <url-pattern>/ShowStatementServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>transactionProcess</servlet-name>
        <url-pattern>/transactionProcess.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
